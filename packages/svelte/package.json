{
  "name": "@caac/svelte",
  "version": "1.0.0",
  "description": "Svelte 5 Cat Image component for CAAC",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "svelte": "./src/index.js"
    },
    "./CatImage.svelte": "./src/CatImage.svelte"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "svelte": "./src/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint src --ext .js,.ts,.svelte",
    "test": "vitest",
    "storybook": "storybook dev -p 6008",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@caac/shared": "workspace:*"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@testing-library/svelte": "^4.0.0",
    "jsdom": "^23.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^3.6.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}