{
  "name": "@caac/shared",
  "version": "1.0.0",
  "description": "Shared utilities and types for CAAC components",
  "keywords": [
    "cat",
    "api",
    "component",
    "utilities",
    "typescript",
    "shared",
    "cross-framework"
  ],
  "author": {
    "name": "CAAC Team",
    "email": "team@caac.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caac-team/caac.git",
    "directory": "packages/shared"
  },
  "homepage": "https://github.com/caac-team/caac#readme",
  "bugs": {
    "url": "https://github.com/caac-team/caac/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}