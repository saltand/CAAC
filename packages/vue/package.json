{
  "name": "@caac/vue",
  "version": "1.0.0",
  "description": "Vue 2/3 compatible Cat Image component for CAAC",
  "keywords": [
    "vue",
    "vue3",
    "component",
    "cat",
    "image",
    "api",
    "typescript",
    "sfc",
    "ui",
    "caac"
  ],
  "author": {
    "name": "CAAC Team",
    "email": "team@caac.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caac-team/caac.git",
    "directory": "packages/vue"
  },
  "homepage": "https://github.com/caac-team/caac#readme",
  "bugs": {
    "url": "https://github.com/caac-team/caac/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.vue",
    "test": "vitest"
  },
  "dependencies": {
    "@caac/shared": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^2.7.0 || ^3.0.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^2.4.0",
    "jsdom": "^23.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "vue": "^3.3.0",
    "vue-tsc": "^1.8.0"
  }
}